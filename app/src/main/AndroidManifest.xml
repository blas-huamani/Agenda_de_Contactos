<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lenovo.agenda">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Agenda">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Nuevo"
            android:label="@string/title_activity_nuevo" />
        <activity
            android:name=".Busqueda"
            android:label="@string/title_activity_busqueda" />
        <activity
            android:name=".Detalles"
            android:label="@string/title_activity_detalles" />
        <activity
            android:name=".Modificar"
            android:label="@string/title_activity_modificar"></activity>
    </application>

</manifest>
